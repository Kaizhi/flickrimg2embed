{

  "manifest_version": 2,

  "name": "Flickr Image to Embedv3",
  "description": "Replace Flickr img tags with Flickr web embeds.",
  "version": "0.1.1",

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "exclude_matches": ["*://*.flickr.com/*"],
      "js": ["fi2e.js"]
    }
  ],
  "web_accessible_resources": ["client-code.js"]


}